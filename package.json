{
	"name": "openbrewerydb-sveltekit",
	"version": "1.0.0",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-netlify": "^1.0.0-next.56",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.16",
		"@typescript-eslint/parser": "^5.16",
		"autoprefixer": "^10",
		"eslint": "^8.12",
		"eslint-config-prettier": "^8.4",
		"eslint-plugin-svelte3": "^3.2",
		"postcss": "^8.4",
		"postcss-load-config": "^3.1",
		"prettier": "~2.6",
		"prettier-plugin-svelte": "^2.6",
		"svelte": "^3.36",
		"svelte-check": "^2.4",
		"svelte-preprocess": "^4.10",
		"tailwindcss": "^3.0",
		"tslib": "^2.3",
		"typescript": "^4.6"
	},
	"type": "module"
}
